      "aarch64-unknown-linux-gnu",
      "aarch64-pc-windows-msvc"
    ]
  },
  "devDependencies": {
    "@napi-rs/cli": "3.0.0-alpha.63",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-esm-shim": "^0.1.7",
    "consola": "^3.2.3",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-copy": "^3.5.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
